{{ form_start(form, {'action': path('employee_education_create'), 'attr': {'class': 'form-horizontal legend', 'id': 'educationForm' } } ) }}

    <div class="control-group">
        <label class="control-label" for="name">School/University</label>
        <div class="controls">
            {{ form_widget(form.name) }}
        </div>
    </div>
    <div class="control-group">
        <label class="control-label" for="start">Start Date</label>
        <div class="controls">
            {{ form_widget(form.start)}}
        </div>
    </div>
    <div class="control-group">
        <label class="control-label" for="end">End Date</label>
        <div class="controls">
            {{ form_widget(form.end) }}
        </div>
    </div>
    <div class="control-group">
        <label class="control-label" for="degree">Degree</label>
        <div class="controls">
            {{ form_widget(form.degree, { 'attr': {'class': 'chzn-select'} } ) }}
        </div>
    </div>
    
    <div class="control-group">
        <label class="control-label" for="field">Field</label>
        <div class="controls">
            {{ form_widget(form.field) }}
        </div>
    </div>
    <div class="control-group">
        <label class="control-label" for="grade">Grade</label>
        <div class="controls">
            {{ form_widget(form.grade) }}
        </div>
    </div>
    <div class="control-group">
        <label class="control-label" for="description">Description</label>
        <div class="controls">
            {{ form_widget(form.description) }}
        </div>
    </div>
    <center>
        {{ form_widget(form.submit, { 'attr': {'class': 'btn bten-large'} } ) }}
    </center>
    <br>

{{ form_end(form) }}
