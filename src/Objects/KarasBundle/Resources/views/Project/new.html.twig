{{ form_start(form, {'action': path('employee_project_create'), 'attr': {'class': 'form-horizontal legend', 'id': 'projectForm' } } ) }}

    <div class="control-group">
        <label class="control-label" for="name">Name</label>
        <div class="controls">
            {{ form_widget(form.name) }}
        </div>
    </div>
    <div class="control-group">
        <label class="control-label" for="occupation">Occupation</label>
        <div class="controls">
            {{ form_widget(form.occupation) }}
        </div>
    </div>
    <div class="control-group">
        <label class="control-label" for="start">Start Date</label>
        <div class="controls">
            {{ form_widget(form.start)}}
        </div>
    </div>
    <div class="control-group">
        <label class="control-label" for="end">End Date</label>
        <div class="controls">
            {{ form_widget(form.end) }}
        </div>
    </div>
    <div class="control-group">
        <label class="control-label" for="description">Description</label>
        <div class="controls">
            {{ form_widget(form.description) }}
        </div>
    </div>
    <center>
        {{ form_widget(form.submit, { 'attr': {'class': 'btn bten-large'} } ) }}
    </center>
    <br>

{{ form_end(form) }}
